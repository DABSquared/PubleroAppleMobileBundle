{
    "name": "publero/apple-mobile-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for handling apple mobile payments and working with appstore tiers.",
    "keywords": ["Apple", "Mobile Payments", "AppStoreTier"],
    "homepage": "http://publero.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Hlaváč",
            "email": "info@mhlavac.net"
            "homepage": "http://www.mhlavac.net/"
        },
        {
            "name": "Publero s.r.o. developers",
            "homepage": "https://www.publero.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.0.0",
        "symfony/yaml": ">=2.0.0",
        "doctrine/doctrine-bundle": ">=1.0.0",
        "stof/doctrine-extensions-bundle": ">=1.1.x-dev",
        "publero/test": "dev-master"
    },
    "suggest": {
        "ext-curl": *
    }
    "require-dev": {
    },
    "autoload": {
        "psr-0": {
            "Publero\\AppleMobileBundle": ""
        }
    },
    "target-dir": "Publero/AppleMobileBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}